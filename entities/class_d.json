{
  "format_version": "1.20.0",
  "minecraft:entity": {
    "description": {
      "identifier": "mod12:class_d",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false,
      "scripts": {
        "animate": [
          "scp002",
          "become_melee",
          "spawn_with_eq"
        ]
      },
      "animations": {
          "scp002": "controller.animation.player.scp002",
          "become_melee": "controller.animation.human.melee",
          "spawn_with_eq": "controller.animation.classd.with_eq"
      }
    },

    "component_groups": {
      "classd:with_eq": {
        "minecraft:is_sheared": {}
      },
      "human:not_melee": {
        "minecraft:behavior.random_stroll": {
          "priority": 5,
          "speed_multiplier": 1
        },
        "minecraft:type_family": {
          "family": ["human", "scpfoundation", "mob", "villager", "class_d", "class_d_female"]
        },
        "minecraft:behavior.panic": {
          "priority": -2,
          "speed_multiplier": 1.25
        },
        "minecraft:movement": {
          "value": 0.26
        },
        "minecraft:behavior.avoid_mob_type": {
          "priority": 3,
          "entity_types": [
            {
              "filters": {
                "any_of": [
                  { "test" :  "is_family", "subject" : "other", "value" :  "scp" },
                  { "test" :  "is_family", "subject" : "other", "value" :  "chaos" },
                  { "test" :  "is_family", "subject" : "other", "value" :  "monster" },
                  { "test" :  "is_family", "subject" : "other", "value" :  "warden" },
                  { "test" :  "is_family", "subject" : "other", "value" :  "herobrine" }
                ]
              },
              "max_dist": 20,
              "sprint_speed_multiplier": 1.6
            }
          ]
        }
      },
      "human:melee": {
        "minecraft:behavior.hurt_by_target": {
          "priority": 2,
          "entity_types": {
            "filters": {
              "any_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "operator": "!=",
                  "value": "d_class"
                }
              ]
            },
            "max_dist": 70
          }
        },
        "minecraft:behavior.random_stroll": {
          "priority": 5,
          "speed_multiplier": 0.8
        },
        "minecraft:type_family": {
          "family": ["human", "scpfoundation", "mob", "villager", "class_d", "class_d_female"]
        },
        "minecraft:attack": {
          "damage": 1
        },
        "minecraft:behavior.melee_attack": {
          "priority": 0
        },
        "minecraft:behavior.nearest_attackable_target": {
          "priority": 1,
          "within_radius": 42,
          "reselect_targets": true,
          "attack_interval": 0.8,
          "entity_types": [
            {
              "filters": {
                "any_of": [
                  { "test" :  "is_family", "subject" : "other", "value" :  "scp" },
                  { "test" :  "is_family", "subject" : "other", "value" :  "chaos" },
                  { "test" :  "is_family", "subject" : "other", "value" :  "monster" },
                  { "test" :  "is_family", "subject" : "other", "value" :  "warden" },
                  { "test" :  "is_family", "subject" : "other", "value" :  "herobrine" }
                ]
              },
              "max_dist": 42
            }
          ],
          "must_see_forget_duration": 15.0,
          "must_see": true
        },
        "minecraft:follow_range": {
          "value": 24,
          "max": 24
        },
        "minecraft:movement": {
          "value": 0.36
        }
      },
      "classd:calm": {
        "minecraft:on_target_acquired": {
          "event": "classd:become_angry",
          "target": "self"
        }
      },
      "classd:angry": {
        "minecraft:angry": 
        {
          "duration": 25,
          "broadcast_anger": true,
          "broadcast_range": 30,
          "broadcast_targets": ["d_class_melee"],
          "calm_event": 
          {
            "event": "classd:on_calm",
            "target": "self"
          }
        }
      },

      "become_zombie": {
        "minecraft:transformation": {
          "into": "minecraft:zombie"
        }
      },
      "minecraft:stopped": {
      "minecraft:economy_trade_table": {
        "display_name": "D Class Personnel",
        "table": "trading/economy_trades/class_d.json",
        "new_screen": true,
        "persist_trades": true
      },
        "minecraft:behavior.random_stroll": {
          "priority": 5,
          "speed_multiplier": 0
        }
      },
      "minecraft:normal": {
      "minecraft:economy_trade_table": {
        "display_name": "D Class Personnel",
        "table": "trading/economy_trades/class_d.json",
        "new_screen": true,
        "persist_trades": true
      },
        "minecraft:behavior.random_stroll": {
          "priority": 5,
          "speed_multiplier": 1
        }
      },
      
      "minecraft:base_classd1": {
        "minecraft:variant": {
          "value": 0
        }
      },
      "minecraft:base_classd2": {
        "minecraft:variant": {
          "value": 1
        }
      },
      "minecraft:base_classd3": {
        "minecraft:variant": {
          "value": 2
        }
      },
      "minecraft:base_classd4": {
        "minecraft:variant": {
          "value": 3
        }
      },
      "minecraft:base_classd5": {
        "minecraft:variant": {
          "value": 4
        }
      }
    },

    "components": {
      "minecraft:behavior.send_event": {
        "priority": 0,
        "event_choices": [
          {
            "min_activation_range": 0.0,
            "max_activation_range": 2.0,
            "cooldown_time": 2.0,
            "cast_duration": 0.1,
            "particle_color": "#FFB38033",
            "weight": 3,
            "filters": { 
                "all_of": [
                  { "test" :  "is_family", "subject" : "other", "value" :  "corridor_door"}
                ] 
            },
            "start_sound_event": "cast.spell",
            "sequence": [
              {
                "base_delay": 0.1,
                "event": "become_zombie",
                "sound_event": "prepare.wololo"
              }
            ]
          }
        ]
      },
      "minecraft:economy_trade_table": {
        "display_name": "D Class Personnel",
        "table": "trading/economy_trades/class_d.json",
        "new_screen": true,
        "persist_trades": true
      },
      "minecraft:damage_sensor": {
        "triggers": 
        [
          {
            "on_damage": {
              "filters": {
                "other_with_families": ["chaos"]
              }
          },
          "deals_damage": false
          },
          {
            "on_damage": {
              "filters": {
                "any_of": [
                  {"test": "is_family", "subject": "other", "value": "zombie"},
                  {"test": "is_family", "subject": "other", "value": "husk"},
                  {"test": "is_family", "subject": "other", "value": "scp049"}
                ],
                "all_of": [
                  {"test": "has_damage", "value": "fatal"}
                ]
              },
              "event": "become_zombie"
            }
          }
        ]
      },
      "minecraft:type_family": {
          "family": ["human", "scpfoundation", "mob", "villager", "class_d", "class_d_female"]
      },
      "minecraft:behavior.equip_item": {
        "priority": 3
      },
      "minecraft:leashable": {
        "soft_distance": 2.0,
        "hard_distance": 3.0,
        "max_distance": 7.0
      },
      "minecraft:health": {
        "value": 20,
		    "max": 20
      },
      "minecraft:attack": {
        "damage": 1
      },
      "minecraft:movement.basic": {
      },
      "minecraft:jump.static": {
      },
      "minecraft:can_climb": {
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:behavior.random_look_around": {
        "priority": 6
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 1.9
      },
      "minecraft:behavior.look_at_player": {
        "priority": 8,
        "look_distance": 4.2,
        "probability": 0.03
      },
      "minecraft:behavior.pickup_items": {
        "priority": 6,
        "max_dist": 3,
        "goal_radius": 2,
        "speed_multiplier": 1.0,
        "pickup_based_on_chance": true,
        "can_pickup_any_item": true
      },
      "minecraft:nameable": {
      },
      "minecraft:physics": {
      },
      "minecraft:conditional_bandwidth_optimization": {
      },
      "minecraft:navigation.walk": {
        "can_float": true,
		    "is_amphibious": false,
		    "can_path_over_water": true,
        "avoid_sun": false,
        "avoid_water": true,
        "avoid_damage_blocks": true,
        "can_pass_doors": false,
        "can_open_doors": false
      }
    },
    "events": {
      "minecraft:entity_spawned": {
        "sequence": [
          {
            "randomize": [
              {
                "weight": 70,
                "add": {
                  "component_groups": [ "minecraft:base_classd1" ]
                }
              },
              {
                "weight": 70,
                "add": {
                  "component_groups": [ "minecraft:base_classd2" ]
                }
              },
              {
                "weight": 70,
                "add": {
                  "component_groups": [ "minecraft:base_classd3" ]
                }
              },
              {
                "weight": 70,
                "add": {
                  "component_groups": [ "minecraft:base_classd4" ]
                }
              },
              {
                "weight": 70,
                "add": {
                  "component_groups": [ "minecraft:base_classd5" ]
                }
              }
            ]
          },
          {
            "add": {
              "component_groups": [
                "human:not_melee",
                "classd:calm"
              ]
            }
          }
        ]
      },
      "classd:on_calm": {
        "remove": {
          "component_groups": [
            "classd:angry"
          ]
        },
        "add": {
          "component_groups": [
            "classd:calm"
          ]
        }
      },
      "classd:become_angry": {
        "add": {
          "component_groups": [
            "classd:angry"
          ]
        },
        "remove": {
          "component_groups": [
            "classd:calm"
          ]
        }
      },
      "minecraft:to_stop": {
        "remove": {
          "component_groups": [
            "minecraft:normal"
          ]
        },
        "add": {
          "component_groups": [
            "minecraft:stopped"
          ]
        }
      },
      "minecraft:to_move": {
        "remove": {
          "component_groups": [
            "minecraft:stopped"
          ]
        },
        "add": {
          "component_groups": [
            "minecraft:normal"
          ]
        }
      },
      "human:become_melee": {
        "add": {
          "component_groups": [
            "human:melee"
          ]
        },
        "remove": {
          "component_groups": [
            "human:not_melee"
          ]
        }
      },
      "human:become_not_melee": {
        "remove": {
          "component_groups": [
            "human:melee"
          ]
        },
        "add": {
          "component_groups": [
            "human:not_melee"
          ]
        }
      },
      "become_zombie": {
        "sequence": [
          {
            "filters": {"test": "is_difficulty", "value": "normal"},
            "randomize": [
              {
                "weight": 130,
                "add": { "component_groups": [ "become_zombie" ] }
              },
              {
                "weight": 130
              }
            ]
          },
          {
            "filters": {"test": "is_difficulty", "value": "hard"},
            "add": {
              "component_groups": [
                "become_zombie"
              ]
            }
          }
        ]
      },
      "classd:with_eq": {
        "sequence": [
          {
            "randomize": [
              {
                "weight": 70,
                "add": {
                  "component_groups": [ "minecraft:base_classd1" ]
                }
              },
              {
                "weight": 70,
                "add": {
                  "component_groups": [ "minecraft:base_classd2" ]
                }
              },
              {
                "weight": 70,
                "add": {
                  "component_groups": [ "minecraft:base_classd3" ]
                }
              },
              {
                "weight": 70,
                "add": {
                  "component_groups": [ "minecraft:base_classd4" ]
                }
              },
              {
                "weight": 70,
                "add": {
                  "component_groups": [ "minecraft:base_classd5" ]
                }
              }
            ]
          },
          {
            "remove": {
              "component_groups": [
                "human:not_melee"
              ]
            },
            "add": {
              "component_groups": [
                "human:melee",
                "classd:with_eq"
              ]
            }
          }
        ]
      }
    }
  }
}
